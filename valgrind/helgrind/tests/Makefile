#	    if [ ! -e $$f.dSYM  -o  $$f -nt $$f.dSYM ] ; then \
#	        echo "dsymutil $$f"; \
#	        dsymutil $$f; \
#	    fi \
#	done

#clean-local:
#	for f in $(check_PROGRAMS) ; do \
#	    rm -rf $$f.dSYM; \
#	done
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
