syntax: glob

# Ignore temp files generated by editors and other tools
*~

# Ignore files generated by hg revert and similar files
*.orig
*.new
*.old

# Ignore files generated by Autotools
Makefile
Makefile*.in
.deps
valgrind/aclocal.m4
valgrind/autom4te.cache
valgrind/configure
valgrind/config.h
valgrind/config.log
valgrind/config.status
valgrind/default.supp
valgrind/glibc-2.X.supp
valgrind/stamp-h1
valgrind/valgrind.pc
valgrind/valgrind.spec
valgrind/valt_load_address_*_linux.lds
VEX/pub/libvex_guest_offsets.h

# Ignore files generated during document generation
design-doc/fjalar.aux
design-doc/fjalar.log
design-doc/fjalar.pdf

# Ignore generated assembly, object and shared library files
*.a
*.o
*.so

# Ignore generated test drivers
.in_place/default.supp
auxprogs/valgrind-listener
cachegrind/cachegrind_annotate
cachegrind/cg_annotate
cachegrind/cg_merge
callgrind/callgrind_annotate
callgrind/callgrind_control
coregrind/no_op_client_for_valgrind
coregrind/valgrind
massif/ms_print
perf/vg_perf
tests/vg_regtest
*-*-linux

# Ignore files generated during test runs
*.stderr.diff
*.stderr.out

# Ignore generated and installed valgrind tools
valgrind/inst

